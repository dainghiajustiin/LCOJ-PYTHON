MAXN = 123

arr = [[0]*MAXN]*MAXN

def init():

    return

def solve():
    init()

    n, m = list(map(int, input().split()))
    lst = list(map(int, input().split()))
    cnt = 0
    S = 0

    for i in range(n):
        for j in range(m):
            if(i&1): S += lst[cnt]
            cnt += 1
    
    print(S)

    return

solve()
