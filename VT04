lst = []
def init():

    return

def solve():
    init()

    n, x = list(map(int, input().split()))
    lst = list(map(int, input().split()))
    ok = False 

    for elm in lst:
        if(elm == x):
            ok = True 
            break 

    if(ok): print("YES")
    else: print("NO")

    return

solve()
