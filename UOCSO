def cnt(n):
    res = 0
    for i in range(1, int(n**0.5) + 1):
        if(n % i == 0):
            res += 1 
            if(i*i != n): res += 1
    return res

def init():

    return

def solve():
    init()

    n = int(input())
    lst = list(map(int, input().split()))

    for elm in lst:
        print(cnt(elm))

    return

solve()
