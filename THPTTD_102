with open('replacedigit.inp', 'r') as file_in:
    content = file_in.readlines()

with open('replacedigit.out', 'w') as file_out:
    nTest = int(content[0])
    for i in range(1, len(content)):
        for j in range(len(content[i])):
            if(content[i][j] == '0'):
                file_out.write('5')
            else:
                file_out.write(content[i][j])
