def init():

    return

def solve():
    init()

    s = input()
    cnt = [0] * 200
    
    s = s.lower()
    for elm in s:
        cnt[ord(elm)] += 1

    s = s.upper()
    for elm in s:
        cnt[ord(elm)] += 1

    T = int(input())

    for _ in range(T):
        c = str(input())
        print(cnt[ord(c)])
    return

solve()
