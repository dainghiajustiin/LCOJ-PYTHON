MAXN = 15

arr = [[0]*MAXN]*MAXN

def init():

    return

def solve():
    init()

    n, m = list(map(int, input().split()))
    lst = list(map(int, input().split()))
    cnt = 0

    for i in range(n):
        for j in range(m):
            print(lst[cnt], end=' ')
            cnt += 1
        print()

    return

solve()
