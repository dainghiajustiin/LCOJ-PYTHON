def init():

    return

def solve():
    init()

    n, S = list(map(int, input().split()))
    lst = list(map(int, input().split()))
    dp = [100000000000] * 100005
    dp[0] = 0
    
    for i in range(1, S+1):
        for j in range(n):
            if(i >= lst[j]):
                dp[i] = min(dp[i], dp[i - lst[j]] + 1)

    print(dp[S] if dp[S] != 100000000000 else -1)

    return

solve()
