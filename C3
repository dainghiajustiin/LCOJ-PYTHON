def init():

    return

def solve():
    init()

    n, k = list(map(int, input().split()))
    lst = list(map(int, input().split()))
    cnt = [0] * 501
    ans = 0

    for i in range(n):
        if(0 <= lst[i] and lst[i] <= 500):
            cnt[lst[i]] += 1
        VALUE = k - lst[i]*lst[i]
        if(0 <= VALUE and VALUE <= 500):
            ans += cnt[VALUE]
    print(ans)    

    return

solve()
