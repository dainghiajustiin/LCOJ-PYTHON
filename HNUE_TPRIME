def is_prime(N):
    if(N < 2): return False
    
    for i in range(2, N):
        if(i*i > N): break
        if(N % i == 0): return False

    return True

with open('TPRIME.inp', 'r', encoding = 'utf-8') as file_in:
    N = int(file_in.readline())

with open('TPRIME.out', 'w', encoding = 'utf-8') as file_out:
    result = 0

    for i in range(N):
        if(i * i > N): break
        if(is_prime(i)): result += 1

    file_out.write(str(result))
