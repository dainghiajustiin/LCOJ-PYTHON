def init():

    return

def solve():
    init()

    n = int(input())
    lst = list(map(int, input().split()))
    lst.sort()

    ansHigh = lst[0]
    ansCnt = 1
    cnt = 1
    for i in range(1, n):
        if(lst[i] == lst[i-1]):
            cnt += 1
        else:
            cnt = 1 
        if(cnt >= ansCnt):
            ansCnt = cnt  
            ansHigh = lst[i]
    
    print(ansHigh, ansCnt)

    return

solve()
