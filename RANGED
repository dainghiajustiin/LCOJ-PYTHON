def init():

    return

def solve():
    init()
    
    lst = list(map(int, input().split()))
    a = lst[0]
    b = lst[1]
    c = lst[2]
    d = lst[3]
    
    if((c <= a and a <= d) or (c <= b and b <= d) or (a <= c and c <= b) or (a <= d and d <= b)): print("YES")
    else: print("NO")

    return

solve()
